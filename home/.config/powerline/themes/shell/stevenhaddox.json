{
  "default_module": "powerline.segments.common",
  "segment_data": {
    "hostname": {
      "before": " ",
      "args": {
        "only_if_ssh": true
      }
    },
    "user": {
      "args": {
        "only_if_ssh": true
      }
    },
    "virtualenv": {
      "before": "ⓔ  "
    },
    "branch": {
      "before": " "
    }
  },
  "segments": {
    "left": [
      {
        "name": "hostname"
      },
      {
        "name": "user"
      },
      {
        "name": "virtualenv"
      },
      {
        "name": "cwd",
        "args": {
          "dir_limit_depth": 3,
          "dir_limit_len": 1
        }
      }
    ],
    "right": [
      {
        "name": "branch"
      },
      {
        "module": "powerline.segments.shell",
        "name": "last_pipe_status"
      }
    ]
  }
}
